<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - MyCoreApp</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/Button1.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/MyCoreApp.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
        <span class="logo">MyCoreApp</span>
        <nav class="text-muted">
            <ul>
                <li><a class="text-light">Home</a>
                    <ul>
                        <li><a class="text-light" asp-area="" asp-controller="Home" asp-action="Index">Encryption</a></li>
                        <li><a class="text-light" asp-area="" asp-controller="Home" asp-action="Decryption">Decryption</a></li>
                    </ul>
                </li>
                <li><a class="text-light">Dowload</a>
                    <ul>
                        <li><a class="text-light" asp-area="" asp-controller="Home" asp-action="Dowload">Dowload_EncryptF</a></li>
                        <li><a class="text-light" asp-area="" asp-controller="Home" asp-action="DowloadDecrypt">Dowload_DecryptF</a></li>
                    </ul>
                </li>
                <li><a class="text-light" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a></li>
            </ul>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    <footer class="border-top text-muted">
        <div class="container">
            &copy; 2023 - MyCoreApp - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>